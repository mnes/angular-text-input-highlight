{
  "name": "angular-text-input-highlight",
  "version": "1.4.3",
  "description": "A component that can highlight parts of text in a textarea. Useful for displaying mentions etc",
  "main": "bundles/angular-text-input-highlight.umd.js",
  "module": "fesm5/angular-text-input-highlight.js",
  "typings": "angular-text-input-highlight.d.ts",
  "lint-staged": {
    "{demo,src,test}/**/*.ts": [
      "npm run prettier",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/prompt"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        0
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattlewis92/angular-text-input-highlight.git"
  },
  "keywords": [
    "angular4",
    "angular2",
    "angular",
    "highlight",
    "textarea"
  ],
  "author": "Matt Lewis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattlewis92/angular-text-input-highlight/issues"
  },
  "homepage": "https://github.com/mattlewis92/angular-text-input-highlight#readme",
  "peerDependencies": {
    "@angular/core": "^9.1.13",
    "@angular/forms": "^9.1.13",
    "@angular/language-service": "^9.1.13",
    "@angular/platform-browser": "^9.1.13",
    "@angular/platform-browser-dynamic": "^9.1.13",
    "@angular/platform-server": "^9.1.13",
    "@angular/router": "^9.1.13"
  },
  "dependencies": {
    "tslib": "^1.10.0"
  },
  "es2015": "fesm2015/angular-text-input-highlight.js",
  "esm5": "esm5/angular-text-input-highlight.js",
  "esm2015": "esm2015/angular-text-input-highlight.js",
  "fesm5": "fesm5/angular-text-input-highlight.js",
  "fesm2015": "fesm2015/angular-text-input-highlight.js",
  "metadata": "angular-text-input-highlight.metadata.json",
  "sideEffects": false
}
